Card from './components/DailyCard'\nimport SummaryCard from './components/SummaryCard'\nimport RegionsCard from './components/RegionsCard'\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      title: 'Covid-19 Italia',\n      fase: 'FASE 3'\n    }\n  },\n  components: {\n    DailyCard,\n    SummaryCard,\n    RegionsCard\n  }\n}\n\nvar numeral = require(\"numeral\");\nVue.filter(\"formatNumber\", function (value) {\n  return numeral(value).format(\"0,0\");\n});\n\nVue.filter(\"formatDate\", function (value) {\n  var today = new Date().getDate();\n  if (value == today) return 'oggi'\n  else return 'ieri'\n});\n</script>\n\n\n<style scoped>\n#app {\n  width: 100%;\n  margin: 0;\n  padding: 0;\n  padding-bottom: 40px;\n  background-color: #000000;\n}\n\nh1 {\n  color: white;\n  font-size: 35px;\n  font-weight: bold;\n  text-align: center;\n  margin: 0;\n  padding-top: 50px;\n}\n\nh3 {\n  margin: 0;\n  text-align: center;\n  color: rgb(72, 215, 237);\n  animation: title-anim 2s infinite;\n  padding-top: 10px;\n  padding-bottom: 50px;\n  font-weight: bold;\n}\n\n#credits {\n  text-align: center;\n}\n\nh4 {\n  color: white;\n  margin-top: 40px;\n  margin-bottom: 25px;\n  text-align: center;\n}\n\nimg {\n  margin: 0 15px;\n  height: 20px;\n}\n\n@keyframes title-anim {\n  0% {\n    color: rgb(72, 215, 237);\n  }\n\n  50% {\n    color: white;\n  }\n}\n</style>\n"]}]}