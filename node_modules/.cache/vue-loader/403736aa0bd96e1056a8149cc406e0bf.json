json/dpc-covid19-ita-andamento-nazionale-latest.json')\n          .then(response => {\n              this.values[0] = {'Infetti' : response.data[0].totale_positivi }\n              this.values[1] = {'Guariti' : response.data[0].dimessi_guariti }\n              this.values[2] = {'Deceduti' : response.data[0].deceduti }\n              console.log(this.values)\n          })\n  },\n}\n</script>\n\n<style scoped>\n#main-data {\n  width: 90%;\n  background-color: blueviolet;\n  margin: 0 auto;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n}\n\ndiv.mini-card {\n  width: 30%;\n  height: max-content;\n  background-color: burlywood;\n}\n\nh3.mini-card-title {\n  text-align: center;\n  margin: 0;\n}\n\nh2.mini-card-value {\n  margin: 0;\n  text-align: center;\n}\n\n</style>"]}]}